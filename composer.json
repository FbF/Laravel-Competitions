{
    "name": "fbf/laravel-competitions",
    "description": "A Laravel 4 package to add simple competitions, with entries tied to a logged in user, to a site",
    "keywords": ["laravel","competitions"],
    "license": "MIT",
    "authors": [
        {
            "name": "Neil Crookes",
            "email": "neil.crookes@fivebyfiveuk.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.*",
        "cviebrock/eloquent-sluggable": "1.0.*",
        "fzaninotto/faker": "1.3.*"
    },
    "autoload": {
        "classmap": [
            "src/controllers",
            "src/migrations",
            "src/models",
            "src/seeds"
        ],
        "psr-0": {
            "Fbf\\LaravelCompetitions\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
